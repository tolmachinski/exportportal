        "start": "encore dev --watch",
        "dev": "npx gulp sassall && encore dev",
        "watch": "encore dev --watch",
        "build": "encore production --progress",
        "prod": "npx gulp && npm run build",
        "dev-server": "encore dev-server",
        "backstop-clear": "rm -rf tests/front_end/backstop/assets/test",
        "backstop-ref": "cd tests/front_end/backstop && npx backstop reference",
        "backstop-test-local": "cd tests/front_end/backstop && npx backstop test local=true",
        "backstop-test-server": "cd tests/front_end/backstop && npx backstop test",
        "backstop-report": "cd tests/front_end/backstop && backstop openReport",
        "backstop-approve": "cd tests/front_end/backstop && npx backstop approve",
        "docs:build:dev": "npx apidoc -i docs/api/resource/ -o docs/api/ -t docs/api/resource/mytemplate -c docs/api/resource/dev/",
        "docs:build:loc": "npx apidoc -i docs/api/resource/ -o docs/api/ -t docs/api/resource/mytemplate -c docs/api/resource/loc/",
